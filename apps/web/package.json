{
  "name": "@whitepine/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm exec dotenv -e .env.local -- sh -c 'next dev -p $DEV_WEB_PORT'",
    "build": "pnpm exec next build",
    "start": "pnpm exec dotenv -e .env.local -- sh -c 'next start -p $PROD_WEB_PORT'",
    "lint": "pnpm exec next lint",
    "lint:fix": "pnpm exec next lint --fix",
    "typecheck": "pnpm exec tsc --noEmit",
    "test": "pnpm exec vitest --run",
    "test:watch": "pnpm exec vitest --watch",
    "test:ui": "pnpm exec vitest --ui",
    "test:coverage": "pnpm exec vitest --coverage",
    "clean": "rm -rf .next out",
    "clean:build": "rm -rf .next",
    "analyze": "ANALYZE=true pnpm exec next build",
    "export": "pnpm exec next build && pnpm exec next export",
    "preview": "pnpm exec next build && pnpm exec dotenv -e .env.local -- sh -c 'next start -p $PROD_WEB_PORT'"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@redux-devtools/extension": "^3.3.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@rjsf/core": "^5.24.13",
    "@rjsf/utils": "^5.24.13",
    "@rjsf/validator-ajv8": "^5.24.13",
    "@types/mdx": "^2.0.13",
    "@whitepine/types": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.544.0",
    "marked": "^16.2.1",
    "mongodb": "^6.19.0",
    "next": "^15.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.0.4",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.5.3",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
