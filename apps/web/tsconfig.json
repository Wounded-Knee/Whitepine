{
  "extends": "../../packages/tsconfig/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@shared/*": [
        "../../packages/types/src/*"
      ],
      "@api/*": [
        "../api/src/*"
      ],
      "@web/*": [
        "./*"
      ],
      "@whitepine/types": [
        "../../packages/types/src/index.ts"
      ],
      "@whitepine/types/*": [
        "../../packages/types/src/*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
