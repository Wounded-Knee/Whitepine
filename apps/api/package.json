{
  "name": "@whitepine/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "pnpm exec tsx watch src/server.ts",
    "build": "pnpm exec tsup",
    "build:migrations": "pnpm exec tsup --config tsup.migrations.config.ts",
    "start": "node dist/server.js",
    "lint": "pnpm exec eslint src --ext .ts,.tsx",
    "lint:fix": "pnpm exec eslint src --ext .ts,.tsx --fix",
    "typecheck": "pnpm exec tsc --noEmit",
    "test": "pnpm exec vitest",
    "test:watch": "pnpm exec vitest --watch",
    "test:coverage": "pnpm exec vitest --coverage",
    "clean": "rm -rf dist migrations",
    "clean:build": "rm -rf dist",
    "migrate:dev": "pnpm exec migrate-mongo up --file config/migrate-mongo-config.js",
    "migrate:prod": "pnpm exec migrate-mongo up --file config/migrate-mongo-config.js --env production",
    "migrate:create": "pnpm exec tsx src/scripts/migration-helper.ts create",
    "migrate:down": "pnpm exec migrate-mongo down --file config/migrate-mongo-config.js",
    "migrate:status": "pnpm exec migrate-mongo status --file config/migrate-mongo-config.js",
    "migrate:list": "pnpm exec tsx src/scripts/migration-helper.ts list",
    "migrate:helper": "pnpm exec tsx src/scripts/migration-helper.ts",
    "db:seed": "pnpm exec tsx src/scripts/seed.ts",
    "db:reset": "pnpm exec tsx src/scripts/reset.ts",
    "db:backup": "pnpm exec tsx src/scripts/backup.ts",
    "db:restore": "pnpm exec tsx src/scripts/restore.ts"
  },
  "dependencies": {
    "@whitepine/types": "workspace:*",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "pino": "^8.17.2",
    "zod": "^3.22.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "cookie-session": "^2.0.0",
    "mongoose": "^8.0.3",
    "rate-limiter-flexible": "^5.0.3",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "dotenv": "^16.3.1",
    "migrate-mongo": "^12.1.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.0.0",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/cookie-session": "^2.0.48",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.6.2",
    "tsup": "^8.0.1",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  }
}
