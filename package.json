{
  "name": "usa",
  "version": "1.0.0",
  "description": "White Pine - Modern Next.js monorepo with TypeScript, API, and deployment automation",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter='@whitepine/*' dev",
    "dev:web": "pnpm --filter='@whitepine/web' dev",
    "dev:api": "pnpm --filter='@whitepine/api' dev",
    "build": "pnpm --recursive build",
    "build:web": "pnpm --filter='@whitepine/web' build",
    "build:api": "pnpm --filter='@whitepine/api' build",
    "build:types": "pnpm --filter='@whitepine/types' build",
    "start": "pnpm --parallel --filter='@whitepine/*' start",
    "start:web": "pnpm --filter='@whitepine/web' start",
    "start:api": "pnpm --filter='@whitepine/api' start",
    "lint": "pnpm --recursive lint",
    "lint:fix": "pnpm --recursive lint:fix",
    "typecheck": "pnpm --recursive typecheck",
    "test": "pnpm --recursive test",
    "test:watch": "pnpm --recursive test:watch",
    "clean": "pnpm --recursive clean && rm -rf node_modules",
    "clean:build": "pnpm --recursive clean:build",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "deps:update": "pnpm update --recursive",
    "deps:check": "pnpm outdated --recursive",
    "deps:audit": "pnpm audit --recursive",
    "workspace:info": "pnpm list --depth=0 --recursive",
    "workspace:graph": "pnpm list --graph --depth=0",
    "deploy:dev": "pnpm --filter='@whitepine/ops' deploy:dev",
    "deploy:prod": "pnpm --filter='@whitepine/ops' deploy:prod",
    "migrate:dev": "pnpm --filter='@whitepine/api' migrate:dev",
    "migrate:prod": "pnpm --filter='@whitepine/api' migrate:prod",
    "db:seed": "pnpm --filter='@whitepine/api' db:seed",
    "db:reset": "pnpm --filter='@whitepine/api' db:reset"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "monorepo",
    "pnpm",
    "api",
    "deployment"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "prettier": "^3.0.0"
  }
}
